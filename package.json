{
  "name": "@marp-team/marp-cli",
  "version": "1.4.0",
  "description": "A CLI interface for Marp and Marpit based converters",
  "license": "MIT",
  "author": {
    "name": "Marp team",
    "url": "https://github.com/marp-team"
  },
  "contributors": [
    {
      "name": "Yuki Hattori",
      "url": "https://github.com/yhatt"
    }
  ],
  "keywords": [
    "marp",
    "markdown",
    "cli",
    "slide",
    "deck",
    "presentation"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/marp-team/marp-cli"
  },
  "engines": {
    "node": ">=12"
  },
  "main": "lib/index.js",
  "types": "types/src/index.d.ts",
  "files": [
    "marp-cli.js",
    "lib/",
    "types/"
  ],
  "bin": {
    "marp": "marp-cli.js"
  },
  "pkg": {
    "scripts": "lib/**/*.js"
  },
  "scripts": {
    "build": "yarn -s clean && rollup -c",
    "build:standalone": "run-s build standalone",
    "check:audit": "yarn audit",
    "check:format": "yarn -s format -c",
    "check:ts": "tsc --noEmit",
    "clean": "rimraf lib",
    "format": "prettier \"**/*.{css,js,jsx,json,md,scss,ts,tsx,yaml,yml}\"",
    "format:write": "yarn -s format --write",
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx --report-unused-disable-directives --cache .",
    "lint:css": "stylelint \"src/**/*.{css,scss}\"",
    "prepack": "npm-run-all --parallel check:* lint:* test:coverage --parallel build types",
    "preversion": "run-p check:* lint:* test:coverage",
    "standalone": "rimraf bin && pkg --out-path ./bin .",
    "standalone:pack": "node ./scripts/pack.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "types": "rimraf types && tsc --declaration --emitDeclarationOnly --outDir types",
    "version": "curl https://raw.githubusercontent.com/marp-team/actions/v1/lib/scripts/version.js | node && git add -A CHANGELOG.md",
    "watch": "rollup -w -c"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.15.0",
    "@rollup/plugin-alias": "^3.1.5",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-typescript": "^8.2.5",
    "@rollup/plugin-url": "^6.1.0",
    "@tsconfig/node12": "^1.0.9",
    "@types/cheerio": "^0.22.30",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/markdown-it": "^12.2.0",
    "@types/node": "~14.17.9",
    "@types/pug": "^2.0.5",
    "@types/supertest": "^2.0.11",
    "@types/ws": "^7.4.7",
    "@types/yargs": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "autoprefixer": "^10.3.1",
    "babel-plugin-transform-rename-import": "^2.3.0",
    "bespoke": "bespokejs/bespoke",
    "builtin-modules": "^3.2.0",
    "chalk": "^4.1.2",
    "cheerio": "^1.0.0-rc.10",
    "chrome-launcher": "^0.14.0",
    "codecov": "^3.8.3",
    "cssnano": "^5.0.7",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-jest": "^24.4.0",
    "express": "^4.17.1",
    "get-stdin": "^9.0.0",
    "globby": "^12.0.1",
    "image-size": "^1.0.0",
    "jest": "^27.0.6",
    "jest-junit": "^12.2.0",
    "nanoid": "^3.1.25",
    "npm-run-all": "^4.1.5",
    "os-locale": "^6.0.0",
    "pdf-lib": "^1.16.0",
    "pkg": "^5.3.1",
    "pkg-up": "^3.1.0",
    "portfinder": "^1.0.28",
    "postcss": "^8.3.6",
    "postcss-url": "^10.1.3",
    "pptxgenjs": "^3.7.1",
    "prettier": "^2.3.2",
    "pug": "^3.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.56.2",
    "rollup-plugin-license": "^2.5.0",
    "rollup-plugin-postcss": "^4.0.1",
    "rollup-plugin-pug": "^1.1.1",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.38.0",
    "strip-ansi": "^7.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-scss": "^3.20.1",
    "supertest": "^6.1.5",
    "tar-stream": "^2.2.0",
    "ts-jest": "^27.0.5",
    "ts-key-enum": "^2.0.7",
    "tslib": "^2.3.1",
    "typed-emitter": "^1.3.1",
    "typescript": "^4.3.5",
    "vhtml": "^2.2.0",
    "wrap-ansi": "^8.0.0",
    "yauzl": "^2.10.0",
    "zip-stream": "^4.1.0"
  },
  "dependencies": {
    "@marp-team/marp-core": "^2.1.1",
    "@marp-team/marpit": "^2.1.1",
    "chokidar": "^3.5.2",
    "cosmiconfig": "^7.0.0",
    "import-from": "^4.0.0",
    "is-wsl": "^2.2.0",
    "puppeteer-core": "10.2.0",
    "serve-index": "^1.9.1",
    "tmp": "^0.2.1",
    "v8-compile-cache": "^2.3.0",
    "ws": "^8.1.0",
    "yargs": "^17.1.1"
  },
  "resolutions": {
    "pug": "^3.0.2",
    "pug-runtime": "^3.0.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
