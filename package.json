{
  "name": "@marp-team/marp-cli",
  "version": "0.23.3",
  "description": "A CLI interface for Marp and Marpit based converters",
  "license": "MIT",
  "author": {
    "name": "Marp team",
    "url": "https://github.com/marp-team"
  },
  "contributors": [
    {
      "name": "Yuki Hattori",
      "url": "https://github.com/yhatt"
    }
  ],
  "keywords": [
    "marp",
    "markdown",
    "cli",
    "slide",
    "deck",
    "presentation"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/marp-team/marp-cli"
  },
  "engines": {
    "node": ">=10"
  },
  "main": "lib/index.js",
  "types": "types/src/index.d.ts",
  "files": [
    "marp-cli.js",
    "lib/",
    "types/"
  ],
  "bin": {
    "marp": "marp-cli.js"
  },
  "pkg": {
    "scripts": "lib/**/*.js"
  },
  "scripts": {
    "build": "yarn -s clean && rollup -c",
    "build:standalone": "run-s build standalone",
    "check:audit": "yarn audit",
    "check:format": "yarn -s format -c",
    "check:ts": "tsc --noEmit",
    "clean": "rimraf lib",
    "format": "prettier \"**/*.{css,js,jsx,json,md,scss,ts,tsx,yaml,yml}\"",
    "format:write": "yarn -s format --write",
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx --report-unused-disable-directives --cache .",
    "lint:css": "stylelint \"src/**/*.{css,scss}\"",
    "prepack": "npm-run-all --parallel check:* lint:* test:coverage --parallel build types",
    "preversion": "run-p check:* lint:* test:coverage",
    "standalone": "rimraf bin && pkg --out-path ./bin .",
    "standalone:pack": "node ./scripts/pack.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "types": "rimraf types && tsc --declaration --emitDeclarationOnly --outDir types",
    "version": "curl https://raw.githubusercontent.com/marp-team/actions/v1/lib/scripts/version.js | node && git add -A CHANGELOG.md",
    "watch": "rollup -w -c"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-replace": "^2.4.1",
    "@rollup/plugin-typescript": "^8.2.0",
    "@rollup/plugin-url": "^6.0.0",
    "@types/cheerio": "^0.22.27",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/node": "~14.14.34",
    "@types/pug": "^2.0.4",
    "@types/supertest": "^2.0.10",
    "@types/ws": "^7.4.0",
    "@types/yargs": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "autoprefixer": "^10.2.5",
    "bespoke": "bespokejs/bespoke",
    "builtin-modules": "^3.2.0",
    "chalk": "^4.1.0",
    "cheerio": "^1.0.0-rc.5",
    "codecov": "^3.8.1",
    "cssnano": "^4.1.10",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.0",
    "file-url": "^3.0.0",
    "get-stdin": "^8.0.0",
    "image-size": "^0.9.5",
    "is-wsl": "^2.2.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "mkdirp": "^1.0.4",
    "nanoid": "^3.1.21",
    "npm-run-all": "^4.1.5",
    "os-locale": "^5.0.0",
    "pkg": "^4.4.9",
    "pkg-up": "^3.1.0",
    "portfinder": "^1.0.28",
    "postcss": "^8.2.8",
    "postcss-url": "^10.1.1",
    "prettier": "^2.2.1",
    "pug": "^3.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.41.2",
    "rollup-plugin-license": "^2.3.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-pug": "^1.1.1",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.32.8",
    "screenfull": "^5.1.0",
    "strip-ansi": "^6.0.0",
    "stylelint": "^13.12.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-scss": "^3.19.0",
    "supertest": "^6.1.3",
    "tar-stream": "^2.2.0",
    "ts-jest": "^26.5.3",
    "ts-keycode-enum": "^1.0.6",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3",
    "vhtml": "^2.2.0",
    "wrap-ansi": "^7.0.0",
    "yauzl": "^2.10.0",
    "zip-stream": "^4.1.0"
  },
  "dependencies": {
    "@marp-team/marp-core": "^1.5.0",
    "@marp-team/marpit": "^1.6.4",
    "chokidar": "^3.5.1",
    "chrome-launcher": "^0.13.4",
    "cosmiconfig": "^7.0.0",
    "express": "^4.17.1",
    "globby": "^11.0.2",
    "import-from": "^3.0.0",
    "pptxgenjs": "^3.4.0",
    "puppeteer-core": "8.0.0",
    "serve-index": "^1.9.1",
    "tmp": "^0.2.1",
    "v8-compile-cache": "^2.3.0",
    "ws": "^7.4.4",
    "yargs": "^16.2.0"
  },
  "resolutions": {
    "pug": "^3.0.2",
    "pug-runtime": "^3.0.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
