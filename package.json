{
  "name": "@marp-team/marp-cli",
  "version": "0.16.1",
  "description": "A CLI interface for Marp and Marpit based converters",
  "license": "MIT",
  "author": {
    "name": "Marp team",
    "url": "https://github.com/marp-team"
  },
  "contributors": [
    {
      "name": "Yuki Hattori",
      "url": "https://github.com/yhatt"
    }
  ],
  "keywords": [
    "marp",
    "markdown",
    "cli",
    "slide",
    "deck",
    "presentation"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/marp-team/marp-cli"
  },
  "engines": {
    "node": ">=8.16.0"
  },
  "main": "lib/marp-cli.js",
  "types": "types/src/marp-cli.d.ts",
  "files": [
    "marp-cli.js",
    "lib/",
    "types/"
  ],
  "bin": {
    "marp": "./marp-cli.js"
  },
  "pkg": {
    "scripts": "lib/**/*.js"
  },
  "scripts": {
    "build": "yarn -s clean && rollup -c",
    "build:standalone": "run-s build standalone",
    "check:audit": "yarn audit",
    "check:format": "yarn -s format -c",
    "check:ts": "tsc --noEmit",
    "clean": "rimraf lib",
    "format": "prettier \"**/*.{css,js,json,md,scss,ts,yaml,yml}\"",
    "format:write": "yarn -s format --write",
    "lint:ts": "tslint \"{src,test}/**/*.ts\"",
    "lint:css": "stylelint \"src/**/*.{css,scss}\"",
    "prepack": "npm-run-all --parallel check:* lint:* test:coverage --parallel build types",
    "preversion": "run-p check:* lint:* test:coverage",
    "standalone": "rimraf bin && pkg --out-path ./bin .",
    "standalone:pack": "node ./scripts/pack.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "types": "rimraf types && tsc --declaration --emitDeclarationOnly --outDir types",
    "version": "curl https://raw.githubusercontent.com/marp-team/actions/v1/lib/scripts/version.js | node && git add -A CHANGELOG.md",
    "watch": "rollup -w -c"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^4.0.0",
    "@rollup/plugin-replace": "^2.2.1",
    "@types/cheerio": "^0.22.14",
    "@types/cosmiconfig": "^6.0.0",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.0.23",
    "@types/jest-plugin-context": "^2.9.2",
    "@types/node": "^12.12.8",
    "@types/pug": "^2.0.4",
    "@types/puppeteer": "^1.20.2",
    "@types/supertest": "^2.0.8",
    "@types/ws": "^6.0.3",
    "@types/yargs": "^13.0.3",
    "autoprefixer": "^9.7.1",
    "bespoke": "bespokejs/bespoke",
    "builtin-modules": "^3.1.0",
    "cheerio": "^1.0.0-rc.3",
    "codecov": "^3.6.1",
    "cssnano": "^4.1.10",
    "image-size": "^0.8.3",
    "jest": "^24.9.0",
    "jest-junit": "^9.0.0",
    "jest-plugin-context": "^2.9.0",
    "nanoid": "^2.1.7",
    "npm-run-all": "^4.1.5",
    "pkg": "^4.4.0",
    "postcss-url": "^8.0.0",
    "prettier": "^1.19.1",
    "pug": "^2.0.4",
    "rimraf": "^3.0.0",
    "rollup": "^1.27.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-pug": "^1.1.1",
    "rollup-plugin-terser": "^5.1.2",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-url": "^2.2.4",
    "sass": "^1.23.6",
    "screenfull": "^5.0.0",
    "stylelint": "^12.0.0",
    "stylelint-config-prettier": "^6.0.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-scss": "^3.12.1",
    "supertest": "^4.0.2",
    "tar-stream": "^2.1.0",
    "ts-jest": "^24.1.0",
    "ts-keycode-enum": "^1.0.6",
    "tslib": "^1.10.0",
    "tslint": "^5.20.1",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.2",
    "yauzl": "^2.10.0",
    "zip-stream": "^2.1.2"
  },
  "dependencies": {
    "@marp-team/marp-core": "^0.15.2",
    "@marp-team/marpit": "^1.4.2",
    "@marp-team/pptx": "^0.1.0",
    "carlo": "^0.9.46",
    "chalk": "^3.0.0",
    "chokidar": "^3.3.0",
    "chrome-launcher": "^0.12.0",
    "cosmiconfig": "^6.0.0",
    "express": "^4.17.1",
    "file-url": "^3.0.0",
    "get-stdin": "^7.0.0",
    "globby": "^10.0.1",
    "import-from": "^3.0.0",
    "is-wsl": "^2.1.1",
    "mkdirp": "^0.5.1",
    "os-locale": "^4.0.0",
    "pkg-up": "^3.1.0",
    "portfinder": "^1.0.25",
    "puppeteer-core": "~2.0.0",
    "serve-index": "^1.9.1",
    "strip-ansi": "^6.0.0",
    "tmp": "^0.1.0",
    "v8-compile-cache": "^2.1.0",
    "wrap-ansi": "^6.2.0",
    "ws": "^7.2.0",
    "yargs": "^15.0.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
