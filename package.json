{
  "name": "@marp-team/marp-cli",
  "version": "0.17.2",
  "description": "A CLI interface for Marp and Marpit based converters",
  "license": "MIT",
  "author": {
    "name": "Marp team",
    "url": "https://github.com/marp-team"
  },
  "contributors": [
    {
      "name": "Yuki Hattori",
      "url": "https://github.com/yhatt"
    }
  ],
  "keywords": [
    "marp",
    "markdown",
    "cli",
    "slide",
    "deck",
    "presentation"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/marp-team/marp-cli"
  },
  "engines": {
    "node": ">=10"
  },
  "main": "lib/marp-cli.js",
  "types": "types/src/marp-cli.d.ts",
  "files": [
    "marp-cli.js",
    "lib/",
    "types/"
  ],
  "bin": {
    "marp": "./marp-cli.js"
  },
  "pkg": {
    "scripts": "lib/**/*.js"
  },
  "scripts": {
    "build": "yarn -s clean && rollup -c",
    "build:standalone": "run-s build standalone",
    "check:audit": "yarn audit",
    "check:format": "yarn -s format -c",
    "check:ts": "tsc --noEmit",
    "clean": "rimraf lib",
    "format": "prettier \"**/*.{css,js,jsx,json,md,scss,ts,tsx,yaml,yml}\"",
    "format:write": "yarn -s format --write",
    "lint:ts": "tslint \"{src,test}/**/*.{ts,tsx}\"",
    "lint:css": "stylelint \"src/**/*.{css,scss}\"",
    "prepack": "npm-run-all --parallel check:* lint:* test:coverage --parallel build types",
    "preversion": "run-p check:* lint:* test:coverage",
    "standalone": "rimraf bin && pkg --out-path ./bin .",
    "standalone:pack": "node ./scripts/pack.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "types": "rimraf types && tsc --declaration --emitDeclarationOnly --outDir types",
    "version": "curl https://raw.githubusercontent.com/marp-team/actions/v1/lib/scripts/version.js | node && git add -A CHANGELOG.md",
    "watch": "rollup -w -c"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-replace": "^2.3.1",
    "@rollup/plugin-typescript": "^4.0.0",
    "@rollup/plugin-url": "^4.0.2",
    "@types/cheerio": "^0.22.16",
    "@types/express": "^4.17.3",
    "@types/jest": "^25.1.4",
    "@types/jest-plugin-context": "^2.9.2",
    "@types/node": "~12.12.30",
    "@types/pug": "^2.0.4",
    "@types/puppeteer": "^2.0.1",
    "@types/supertest": "^2.0.8",
    "@types/ws": "^7.2.2",
    "@types/yargs": "^15.0.4",
    "autoprefixer": "^9.7.4",
    "bespoke": "bespokejs/bespoke",
    "builtin-modules": "^3.1.0",
    "cheerio": "^1.0.0-rc.3",
    "codecov": "^3.6.1",
    "cssnano": "^4.1.10",
    "image-size": "^0.8.3",
    "jest": "^25.1.0",
    "jest-junit": "^10.0.0",
    "jest-plugin-context": "^2.9.0",
    "nanoid": "^2.1.11",
    "npm-run-all": "^4.1.5",
    "pkg": "^4.4.4",
    "postcss-url": "^8.0.0",
    "prettier": "^1.19.1",
    "pug": "^2.0.4",
    "rimraf": "^3.0.2",
    "rollup": "^2.0.6",
    "rollup-plugin-postcss": "^2.4.1",
    "rollup-plugin-pug": "^1.1.1",
    "rollup-plugin-terser": "^5.3.0",
    "sass": "^1.26.3",
    "screenfull": "^5.0.2",
    "stylelint": "^13.2.1",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.15.0",
    "supertest": "^4.0.2",
    "tar-stream": "^2.1.2",
    "ts-jest": "^25.2.1",
    "ts-keycode-enum": "^1.0.6",
    "tslib": "^1.11.1",
    "tslint": "^6.1.0",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3",
    "vhtml": "^2.2.0",
    "yauzl": "^2.10.0",
    "zip-stream": "^2.1.3"
  },
  "dependencies": {
    "@marp-team/marp-core": "^1.1.0",
    "@marp-team/marpit": "^1.5.1",
    "carlo": "^0.9.46",
    "chalk": "^3.0.0",
    "chokidar": "^3.3.1",
    "chrome-launcher": "^0.13.0",
    "cosmiconfig": "^6.0.0",
    "express": "^4.17.1",
    "file-url": "^3.0.0",
    "get-stdin": "^7.0.0",
    "globby": "^11.0.0",
    "import-from": "^3.0.0",
    "is-wsl": "^2.1.1",
    "mkdirp": "^1.0.3",
    "os-locale": "^4.0.0",
    "pkg-up": "^3.1.0",
    "portfinder": "^1.0.25",
    "pptxgenjs": "^3.1.1",
    "puppeteer-core": "~2.1.1",
    "serve-index": "^1.9.1",
    "strip-ansi": "^6.0.0",
    "tmp": "^0.1.0",
    "v8-compile-cache": "^2.1.0",
    "wrap-ansi": "^6.2.0",
    "ws": "^7.2.3",
    "yargs": "^15.3.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
